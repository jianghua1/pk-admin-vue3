{
  "name": "pk-front-vue3",
  "version": "1.0.0",
  "private": true,
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "cross-env ELECTRON=true vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:electron": "cross-env ELECTRON=true BASE_PATH=./ npm run build && electron-builder",
    "build:electron-only": "electron-builder",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "analysis": "cross-env ANALYSIS=true npm run p",
    "build:debug": "cross-env SOURCE_MAP=true npm run p",
    "p": "run-s build-only preview"
  },
  "dependencies": {
    "@unocss/reset": "^0.57.4",
    "@vueuse/core": "^10.6.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "el-admin-components": "1.1.2",
    "element-plus": "^2.8.4",
    "gsap": "^3.12.5",
    "howler": "^2.2.4",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^3.2.0",
    "sortablejs": "^1.15.3",
    "vditor": "^3.10.6",
    "video.js": "^8.17.4",
    "vue": "^3.5.10",
    "vue-echarts": "^6.6.5",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.254",
    "@iconify/vue": "^4.1.2",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.9.0",
    "@unocss/preset-wind": "^0.57.2",
    "@unocss/transformer-directives": "^0.58.0",
    "@unocss/transformer-variant-group": "^0.57.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.0",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.79.4",
    "start-server-and-test": "^2.0.8",
    "tree-kill": "^1.2.2",
    "typescript": "~5.2.2",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.4.8",
    "vite-plugin-cdn2": "^1.1.0",
    "vite-plugin-electron": "^0.28.8",
